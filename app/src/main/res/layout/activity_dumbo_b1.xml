<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contact_main"
    android:background="#000000"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusableInTouchMode="true"
    tools:context=".DumboB1Activity" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#000000" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="30dp"
            style="@style/ReserveMessage" />

        <TextView
            android:id="@+id/dates"
            style="@style/SelectLabels"
            android:text="@string/choose_day" />

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingLeft="12dp"
            android:paddingRight="2dp"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:background="#FFFFFF" >

            <Spinner
                android:id="@+id/spin_date"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center" />

        </FrameLayout>

        <TextView
            android:id="@+id/times"
            style="@style/SelectLabels"
            android:text="@string/choose_time" />

        <FrameLayout
            android:id="@+id/frame_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="20dp"
            android:paddingLeft="12dp"
            android:paddingRight="2dp"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:background="#FFFFFF" >

            <Spinner
                android:id="@+id/spin_time"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center" />

        </FrameLayout>

        <TextView
            android:id="@+id/seats"
            style="@style/SelectLabels"
            android:text="@string/choose_seats" />

        <TextView
            style="@style/LabelScreen"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center" >

            <Button
                android:id="@+id/dbcA1"
                style="@style/CustomButtonSeats"
                android:text="A1"/>

            <Button
                android:id="@+id/dbcA2"
                style="@style/CustomButtonSeats"
                android:text="A2"/>

            <Button
                android:id="@+id/dbcA3"
                style="@style/CustomButtonSeats"
                android:text="A3"/>

            <Button
                android:id="@+id/dbcA4"
                style="@style/CustomButtonSeats"
                android:text="A4"/>

            <Button
                android:id="@+id/dbcA5"
                style="@style/CustomButtonSeats"
                android:text="A5"/>

            <Button
                android:id="@+id/dbcA6"
                style="@style/CustomButtonSeats"
                android:text="A6"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center" >

            <Button
                android:id="@+id/dbcB1"
                style="@style/CustomButtonSeats"
                android:text="B1"/>

            <Button
                android:id="@+id/dbcB2"
                style="@style/CustomButtonSeats"
                android:text="B2"/>

            <Button
                android:id="@+id/dbcB3"
                style="@style/CustomButtonSeats"
                android:text="B3"/>

            <Button
                android:id="@+id/dbcB4"
                style="@style/CustomButtonSeats"
                android:text="B4"/>

            <Button
                android:id="@+id/dbcB5"
                style="@style/CustomButtonSeats"
                android:text="B5"/>

            <Button
                android:id="@+id/dbcB6"
                style="@style/CustomButtonSeats"
                android:text="B6"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center" >

            <Button
                android:id="@+id/dbcC1"
                style="@style/CustomButtonSeats"
                android:text="C1"/>

            <Button
                android:id="@+id/dbcC2"
                style="@style/CustomButtonSeats"
                android:text="C2"/>

            <Button
                android:id="@+id/dbcC3"
                style="@style/CustomButtonSeats"
                android:text="C3"/>

            <Button
                android:id="@+id/dbcC4"
                style="@style/CustomButtonSeats"
                android:text="C4"/>

            <Button
                android:id="@+id/dbcC5"
                style="@style/CustomButtonSeats"
                android:text="C5"/>

            <Button
                android:id="@+id/dbcC6"
                style="@style/CustomButtonSeats"
                android:text="C6"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center" >

            <Button
                android:id="@+id/dbcD1"
                style="@style/CustomButtonSeats"
                android:text="D1"/>

            <Button
                android:id="@+id/dbcD2"
                style="@style/CustomButtonSeats"
                android:text="D2"/>

            <Button
                android:id="@+id/dbcD3"
                style="@style/CustomButtonSeats"
                android:text="D3"/>

            <Button
                android:id="@+id/dbcD4"
                style="@style/CustomButtonSeats"
                android:text="D4"/>

            <Button
                android:id="@+id/dbcD5"
                style="@style/CustomButtonSeats"
                android:text="D6"/>

            <Button
                android:id="@+id/dbcD6"
                style="@style/CustomButtonSeats"
                android:text="D6"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center" >

            <Button
                android:id="@+id/dbcE1"
                style="@style/CustomButtonSeats"
                android:text="E1"/>

            <Button
                android:id="@+id/dbcE2"
                style="@style/CustomButtonSeats"
                android:text="E2"/>

            <Button
                android:id="@+id/dbcE3"
                style="@style/CustomButtonSeats"
                android:text="E3"/>

            <Button
                android:id="@+id/dbcE4"
                style="@style/CustomButtonSeats"
                android:text="E4"/>

            <Button
                android:id="@+id/dbcE5"
                style="@style/CustomButtonSeats"
                android:text="E5"/>

            <Button
                android:id="@+id/dbcE6"
                style="@style/CustomButtonSeats"
                android:text="E6"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center" >

            <Button
                android:id="@+id/dbcF1"
                style="@style/CustomButtonSeats"
                android:text="F1"/>

            <Button
                android:id="@+id/dbcF2"
                style="@style/CustomButtonSeats"
                android:text="F2"/>

            <Button
                android:id="@+id/dbcF3"
                style="@style/CustomButtonSeats"
                android:text="F3"/>

            <Button
                android:id="@+id/dbcF4"
                style="@style/CustomButtonSeats"
                android:text="F4"/>

            <Button
                android:id="@+id/dbcF5"
                style="@style/CustomButtonSeats"
                android:text="F5"/>

            <Button
                android:id="@+id/dbcF6"
                style="@style/CustomButtonSeats"
                android:text="F6"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:layout_gravity="center">

            <TextView
                style="@style/GreenBox" />

            <TextView
                style="@style/Vacant" />

            <TextView
                style="@style/RedBox"/>

            <TextView
                style="@style/Occupied" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center">

            <TextView
                style="@style/NumberSeats" />

            <TextView
                android:id="@+id/quantity"
                style="@style/QuantitySeats" />

        </LinearLayout>

        <Button
            style="@style/StandardButton"
            android:text="@string/reserve_seats"
            android:onClick="openResultPage" />

    </LinearLayout>

</ScrollView>